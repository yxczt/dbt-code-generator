{
  "name": "reverseword",
  "displayName": "ReverseWord",
  "description": "Reverse a word",
  "version": "0.0.1",
  "publisher": "rebornix",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.dbt-gen-stg",
        "title": "Generate staging model with ChatGPT"
      },
      {
        "command": "extension.dbt-gen-insight",
        "title": "Generate insightful models with ChatGPT"
      },
      {
        "command": "extension.dbt-gen-stg-yaml",
